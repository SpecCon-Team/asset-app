{
  "name": "asset-app",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "dev": "npm run dev -w client",
    "build": "npm run build -w client && npm run build -w server",
    "preview": "npm run preview -w client",
    "server:dev": "npm run dev -w server",
    "server:migrate": "npm run prisma:migrate -w server",
    "server:generate": "npm run prisma:generate -w server",
    "lint": "npm run lint -w client",
    "test": "npm run test -w client"
  }
}
